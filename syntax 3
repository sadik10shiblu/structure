
#include<stdio.h>
int main(){

struct person
{
    int age;
    float salary;

};
struct person person1={27,25556.36};//direct assign
struct person person2,person3;
//element wise assign
person2.age=25;
person2.salary=252568.36;


//structure wise assignment
person3=person2;  //akta structure ar moddeh arekta structure value assign

 printf("person1: \n");
 printf("age=%d \n",person1.age);
 printf("salary=%.2f \n",person1.salary);



 printf("person2: \n");
 printf("age=%d \n",person2.age);
 printf("salary=%.2f \n",person2.salary);


 printf("person3: \n");
 printf("age=%d \n",person3.age);
 printf("salary=%.2f \n",person3.salary);
return 0;
}
